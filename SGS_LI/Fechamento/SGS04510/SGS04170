003120     05      WTB-DDNS.                                            00000218
003130      10     FILLER          OCCURS  20 TIMES                     00000219
003140                             INDEXED BY WIN-DDN.                  00000220
003150       15    WTB-DDN         PIC X(008).                          00000221
003160       15    WTB-DSN         PIC X(044).                          00000222
003170/*****************************************************************00000223
003180*                P R O C E D U R E    D I V I S I O N            *00000224
003190******************************************************************00000225
003200 PROCEDURE   DIVISION.                                            00000226
003210 000-00-MODULO-DECISAO  SECTION.                                  00000227
003220*                                                                 00000228
003230     PERFORM 010-00-INICIALIZA.                                   00000229
003240*                                                                 00000230
003250     PERFORM 040-00-PROCESSA-REG                                  00000231
003260         UNTIL  WCH-01  EQUAL  HIGH-VALUE.                        00000232
003270*                                                                 00000233
003280     PERFORM 170-00-FINALIZA.                                     00000234
003290*                                                                 00000235
003300     STOP    RUN.                                                 00000236
003310*                                                                 00000237
003320  000-99-MODULO-DECISAO.                                          00000238
003330     EXIT.                                                        00000239
003340/*****************************************************************00000240
003350*                PROCEDIMENTOS INICIAIS DO PROGRAMA              *00000241
003360******************************************************************00000242
003370 010-00-INICIALIZA      SECTION.                                  00000243
003380*                                                                 00000244
003390     OPEN    INPUT  SEQENT01                                      00000245
003400             OUTPUT SEQSAI01                                      00000246
003410                    SEQSAI02                                      00000246
003420                    SEQSAI03.                                     00000246
003430*                                                                 00000247
003440     MOVE    'N'           TO     WCH-EOF.                        00000248
003450     CALL    'DATA'        USING  WRC-DTPROC.                     00000249
003460     ACCEPT   WAX-HORAS    FROM   TIME.                           00000250
003470     MOVE     WAX-HHMMSS   TO     WRC-HORAS.                      00000251
003480     CALL    'CPD10170'    USING  WRC-NOMJOB.                     00000252
003490     CALL    'CPD0003S'    USING  WTB-DDNS.                       00000253
003500*                                                                 00000254
003510     PERFORM  020-00-PROCURA-DSNAMES                              00000255
003520              VARYING  WIN-DDN  FROM  1  BY  1                    00000256
003530              UNTIL    WIN-DDN  GREATER  20  OR                   00000257
003540              WTB-DDN (WIN-DDN)  EQUAL    HIGH-VALUE.             00000258
003550*                                                                 00000259
003560     IF  WCH-STA  NOT EQUAL   '00'                                00000260
003570         MOVE  WCH-STA  TO  WAX-STA                               00000261
003580         MOVE  WAX-MSGIND    TO WRC-OBS                           00000262
003590         PERFORM  160-00-ABEND                                    00000263
003600     END-IF                                                       00000264
003610*                                                                 00000265
003620     PERFORM 030-00-LER-SEQENT01.                                 00000266
003630*                                                                 00000267
003640 010-99-INICIALIZA.                                               00000268
003650     EXIT.                                                        00000269
003660/*****************************************************************00000270
003670*                MONTAGEM  DA  TABELA  DE  DSN                   *00000271
003680******************************************************************00000272
003690 020-00-PROCURA-DSNAMES      SECTION.                             00000273
003700*''                                                               00000274
003710     IF  WTB-DDN (WIN-DDN) EQUAL 'SEQENT01'                       00000275
003720         MOVE WTB-DSN (WIN-DDN) TO WRC-CODE1                      00000276
003730      ELSE                                                        00000277
003740       IF  WTB-DDN (WIN-DDN) EQUAL 'SEQSAI01'                     00000278
003750           MOVE WTB-DSN (WIN-DDN) TO WRC-CODS1                    00000279
003760        ELSE                                                      00000277
003770         IF  WTB-DDN (WIN-DDN) EQUAL 'SEQSAI02'                   00000278
003780             MOVE WTB-DSN (WIN-DDN) TO WRC-CODS2                  00000279
003790          ELSE                                                    00000277
003800           IF  WTB-DDN (WIN-DDN) EQUAL 'SEQSAI03'                 00000278
003810               MOVE WTB-DSN (WIN-DDN) TO WRC-CODS3                00000279
003820           END-IF                                                 00000280
003830         END-IF                                                   00000280
003840       END-IF                                                     00000280
003850     END-IF.                                                      00000281
003860*                                                                 00000282
003870 020-99-PROCURA-DSNAMES.                                          00000283
003880     EXIT.                                                        00000284
003890/*****************************************************************00000285
003900*       ROTINA DE LEITURA DO SEQENT01                            *00000286
003910******************************************************************00000287
003920 030-00-LER-SEQENT01   SECTION.                                   00000288
003930*                                                                 00000289
003940     READ   SEQENT01                                              00000290
003950         AT END                                                   00000291
003960            MOVE   HIGH-VALUE    TO  WCH-01                       00000292
003970            GO                   TO  030-99-LER-SEQENT01.         00000293
003980*                                                                 00000294
003990     ADD    1                    TO  WRC-VALE1.                   00000295
